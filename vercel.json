{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 10,
      "excludeFiles": "{api/tests/**,**/*.test.py,**/test_*.py,api/tests/fixtures/**,**/__pycache__/**,**/*.pyc}"
    }
  },
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek-api-key"
  }
}
