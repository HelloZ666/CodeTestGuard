{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "installCommand": "pip install -r requirements.txt",
        "forwardEnv": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.js)$",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*\\.css)$",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*\\.(ico|png|jpg|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024,
      "excludeFiles": "{api/tests/**,**/*.test.py,api/tests/fixtures/**,**/__pycache__/**,api/.coverage,api/backend.log,api/frontend.log}"
    }
  },
  "env": {
    "PYO3_USE_ABI3_FORWARD_COMPATIBILITY": "1",
    "PYTHONPATH": "."
  },
  "install": {
    "runtime": "python3.11"
  }
}
