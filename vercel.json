{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 10,
      "excludeFiles": "{api/tests/**,**/*.test.py,**/test_*.py,api/tests/fixtures/**,**/__pycache__/**,**/*.pyc}"
    }
  },
  "env": {
    "PYO3_USE_ABI3_FORWARD_COMPATIBILITY": "1"
  }
}
